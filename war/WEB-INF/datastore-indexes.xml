<?xml version="1.0" encoding="utf-8"?>
<datastore-indexes xmlns="http://appengine.google.com/ns/datastore-indexes/1.0" autoGenerate="false">

	<datastore-index kind="SingleMatch" ancestor="false">
		<property name="matchNumber" direction="desc" />
		<property name="matchDate" direction="asc" />
		<property name="matchComment" direction="asc" />
		<property name="player1" direction="asc" />
		<property name="player2" direction="asc" />
		<property name="matchPointsTeam1" direction="asc" />
		<property name="matchPointsTeam2" direction="asc" />
		<property name="matchSetsTeam1" direction="asc" />
		<property name="matchSetsTeam2" direction="asc" />
	</datastore-index>

	<datastore-index kind="DoubleMatch" ancestor="false">
		<property name="matchNumber" direction="desc" />
		<property name="matchDate" direction="asc" />
		<property name="matchComment" direction="asc" />
		<property name="team1" direction="asc" />
		<property name="team2" direction="asc" />
		<property name="matchPointsTeam1" direction="asc" />
		<property name="matchPointsTeam2" direction="asc" />
		<property name="matchSetsTeam1" direction="asc" />
		<property name="matchSetsTeam2" direction="asc" />
	</datastore-index>

	<datastore-index kind="MatchComment" ancestor="false">
		<property name="comment" direction="asc" />
	</datastore-index>

	<datastore-index kind="Player" ancestor="false">
		<property name="lastName" direction="asc" />
		<property name="firstName" direction="asc" />
		<property name="nickName" direction="asc" />
		<property name="eMail" direction="asc" />
		<property name="lastMatchDate" direction="asc" />
		<property name="playerSingleStats" direction="asc" />
		<property name="playerDoubleStats" direction="asc" />
		<property name="teams" direction="asc" />
	</datastore-index>

	<datastore-index kind="PlayerSingleStats" ancestor="false">
		<property name="wins" direction="asc" />
		<property name="defeats" direction="asc" />
		<property name="winSets" direction="asc" />
		<property name="lostSets" direction="asc" />
		<property name="shotGoals" direction="asc" />
		<property name="getGoals" direction="asc" />
		<property name="prevTablePlace" direction="asc" />
		<property name="curTablePlace" direction="asc" />
		<property name="lastMatchPoints" direction="asc" />
		<property name="points" direction="desc" />
		<property name="tendency" direction="asc" />
	</datastore-index>

	<datastore-index kind="PlayerDoubleStats" ancestor="false">
		<property name="wins" direction="asc" />
		<property name="defeats" direction="asc" />
		<property name="winSets" direction="asc" />
		<property name="lostSets" direction="asc" />
		<property name="shotGoals" direction="asc" />
		<property name="getGoals" direction="asc" />
		<property name="prevTablePlace" direction="asc" />
		<property name="curTablePlace" direction="asc" />
		<property name="lastMatchPoints" direction="asc" />
		<property name="points" direction="desc" />
		<property name="tendency" direction="asc" />
	</datastore-index>

	<datastore-index kind="Team" ancestor="false">
		<property name="lastMatchDate" direction="asc" />
		<property name="teamStats" direction="asc" />
		<property name="player1" direction="asc" />
		<property name="player2" direction="asc" />
	</datastore-index>

	<datastore-index kind="TeamStats" ancestor="false">
		<property name="wins" direction="asc" />
		<property name="defeats" direction="asc" />
		<property name="winSets" direction="asc" />
		<property name="lostSets" direction="asc" />
		<property name="shotGoals" direction="asc" />
		<property name="getGoals" direction="asc" />
		<property name="prevTablePlace" direction="asc" />
		<property name="curTablePlace" direction="asc" />
		<property name="lastMatchPoints" direction="asc" />
		<property name="points" direction="desc" />
		<property name="tendency" direction="asc" />
	</datastore-index>

</datastore-indexes>